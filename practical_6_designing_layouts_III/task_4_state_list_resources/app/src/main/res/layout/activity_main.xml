<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.tomek.task_4_state_list_resources.MainActivity"
    android:background="@android:color/black"
    android:orientation="vertical"
    >

    <ImageButton
        android:id="@+id/andbtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

        android:layout_marginTop="30dp"
        android:background="@drawable/butto_selector"
        />

    <Button
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/button_s"
        android:text="Hello World, CustomButtonActivity"
        />



</LinearLayout>
